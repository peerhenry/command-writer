      "filename": "trello.bat",
      "content": "start \"\" \"https://trello.com/\""
    },

    "tweakers" : {
      "filename": "tweakers.bat",
      "content": [
        "@echo off",
        "if \"%1\"==\"\" (",
        "\tstart \"\" \"https://tweakers.net\"",
        ") else (",
        "\tstart \"\" \"https://tweakers.net/zoeken/?keyword=%*\"",
        ")"
      ]
    },

    "woorden" : {
      "filename": "woorden.bat",
      "content": "start \"\" \"http://www.woorden.org/woord/%*\""
    },

    "dictionary" : {
      "filename": "dic.bat",
      "content": "start \"\" \"http://www.dictionary.com/browse/%*?s=t\""
    },

    "imdb" : {
      "filename": "imdb.bat",
      "content": [
        "@echo off",
        "if \"%1\"==\"\" (",
        "\tstart \"\" \"https://www.imdb.com\"",
        ") else (",
        "\tstart \"\" \"http://www.imdb.com/find?ref_=nv_sr_fn&q=%*&s=all\"",
        ")"
      ]
    },

    "wikipedia" : {
      "filename": "wiki.bat",
      "content": "start \"\" \"https://en.wikipedia.org/wiki/%*\""
    },

    "youtube" : {
      "filename": "youtube.bat",
      "content": "start \"\" \"https://www.youtube.com/results?search_query=%*\""
    },

    "trends" : {
      "filename": "trends.bat",
