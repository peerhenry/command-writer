      "filename": "github.bat",
      "content": "start \"\" \"https://github.com\""
    },

    "google" : {
      "filename": "google.bat",
      "content": "start \"\" \"https://www.google.com/search?q=%*\""
    },

    "gmail" : {
      "filename": "gmail.bat",
      "content": "start \"\" \"https://mail.google.com/mail/u/0/#inbox\""
